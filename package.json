{
    "authors": [
        "Paul Gilchrist"
    ],
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "description": "Dungeons and Dragons Character Sheet",
    "homepage": "https://paulgilchrist.github.io/dnd-char-sheet",
    "license": "MIT",
    "name": "dnd-char-sheet",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/PaulGilchrist/dnd-char-sheet.git"
    },
    "scripts": {
        "deploy": "gh-pages -d dist",
        "dev": "concurrently \"npm run dev:react\" \"npm run dev:node\"",
        "dev:react": "vite --host",
        "dev:node": "node server.js",
        "build": "vite build",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "predeploy": "npm run build",
        "preview": "vite preview"
    },
    "type": "module",
    "version": "0.0.0",
    "dependencies": {
        "concurrently": "^8.2.2",
        "express": "^4.18.2",
        "file-saver": "^2.0.5",
        "guid": "^0.0.12",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.55.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "gh-pages": "^6.1.1",
        "vite": "^5.0.8"
    }
}
